{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.4",
        "fourstacks/nova-checkboxes": "^0.1.5",
        "gregoriohc/laravel-nova-theme-responsive": "^0.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/helpers": "^1.1",
        "laravel/nova": "^3.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/vapor-core": "^2.10",
        "laravel/vapor-ui": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "leping/lesson-link": "*",
        "leping/parrish-bull-sharks": "*",
        "leping/swim-school-theme": "*",
        "opanegro/nova-custom-controller": "*",
        "stripe/stripe-php": "^7.0",
        "symfony/translation": "^5.0",
        "tightenco/nova-stripe": "^0.2.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Http/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-custom-controller"
        },
        {
            "type": "path",
            "url": "./nova-components/ParrishBullSharks"
        },
        {
            "type": "path",
            "url": "./nova-components/LessonLink"
        },
        {
            "type": "path",
            "url": "./nova-components/SwimSchoolTheme"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vapor-ui:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan nova:publish --ansi"
        ]
    }
}
