{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3.0",
        "doctrine/dbal": "^2.9",
        "facade/ignition": "^2.3.6",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "^4.0",
        "fourstacks/nova-checkboxes": "^0.1.5",
        "gregoriohc/laravel-nova-theme-responsive": "^0.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/nova": "^3.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "leping/lesson-link": "*",
        "leping/northriverrapids": "*",
        "leping/the-swim-school": "*",
        "opanegro/nova-custom-controller": "^1.5",
        "stripe/stripe-php": "^6.4",
        "symfony/translation": "^5.0",
        "tightenco/nova-stripe": "^0.2.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NorthRiverRapids"
        },
        {
            "type": "path",
            "url": "./nova-components/LessonLink"
        },
        {
            "type": "path",
            "url": "./nova-components/TheSwimSchool"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
